<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/Life_Framlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <WebView
            android:id="@+id/Life_WebView"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </WebView>

        <RelativeLayout
            android:id="@+id/WebView_Relative"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/red">

            <ImageView
                android:id="@+id/Life_Webview_Back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="6dp"
                android:padding="5dp"
                android:src="@drawable/ccoo_icon_back" />

            <TextView
                android:id="@+id/Life_WebView_Guanbi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:layout_toRightOf="@+id/Life_Webview_Back"
                android:padding="5dp"
                android:text="关闭"
                android:textColor="@color/white"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/Life_WebView_Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="房屋出租"
                android:textColor="@color/white"
                android:textSize="20dp" />

            <ImageView
                android:id="@+id/Life_WebView_Shared"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/new_con_share_2" />

            <ProgressBar
                android:id="@+id/Life_WebView_Progress"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/Life_WebView_Title"
                android:background="@drawable/webview_progress"
                />
        </RelativeLayout>


    </FrameLayout>


</RelativeLayout>